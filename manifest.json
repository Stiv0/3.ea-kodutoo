{
  "manifest_version": 2,
 
  "name": "Color Changer",
  "version": "0.1.0",
  "description": "Change the color of your favorite websites",
 
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["jquery-3.2.1.min.js", "content.js"],
    "matches": ["*://*.youtube.com/*"]
  }],
  
  "background": {
    "scripts": ["background.js"]
  },

 "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
     "activeTab",
     "tabs",
     "http://*/*",
     "https://*/*",
     "https://ajax.googleapis.com/"
   ]
 
}